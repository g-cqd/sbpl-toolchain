{
  "comments": {
    "lineComment": ";",
    "blockComment": ["#|", "|#"]
  },
  "brackets": [
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "#|", "close": "|#" }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\(",
      "end": "^\\s*\\)"
    }
  }
}
